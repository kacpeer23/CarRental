@model List<CarRental.Models.ClientModel> 
 <html>  
<head>  
<title>Index</title>  
<style type="text/css">
   table.greenTable {
  font-family: Georgia, serif;
  border: 6px solid ##FFB100;
  background-color: #D4EED1;
  width: 100%;
  text-align: center;
}
table.greenTable td, table.greenTable th {
  border: 1px solid ##FFB100;
  padding: 3px 2px;
}
table.greenTable tbody td {
  font-size: 13px;
}
table.greenTable thead {
  background: #FFB100;
  background: -moz-linear-gradient(top, #5baf6b 0%, #3a9e4d 66%, ##FFB100 100%);
  background: -webkit-linear-gradient(top, #5baf6b 0%, #3a9e4d 66%, ##FFB100 100%);
  background: linear-gradient(to bottom, #5baf6b 0%, #3a9e4d 66%, ##FFB100 100%);
  border-bottom: 0px solid #444444;
}
table.greenTable thead th {
  font-size: 19px;
  font-weight: bold;
  color: #000000;
  text-align: center;
  border-left: 2px solid ##FFB100;
}
table.greenTable thead th:first-child {
  border-left: none;
}
body {
    background-color: #37423D;
}
table.greenTable tfoot {
  font-size: 13px;
  font-weight: bold;
  color: #F0F0F0;
  background: ##FFB100;
  background: -moz-linear-gradient(top, #5baf6b 0%, #3a9e4d 66%, ##FFB100 100%);
  background: -webkit-linear-gradient(top, #5baf6b 0%, #3a9e4d 66%, ##FFB100 100%);
  background: linear-gradient(to bottom, #5baf6b 0%, #3a9e4d 66%, ##FFB100 100%);
  border-top: 1px solid ##FFB100;
}
table.greenTable tfoot td {
  font-size: 13px;
}
table.greenTable tfoot .links {
  text-align: right;
}
table.greenTable tfoot .links a{
  display: inline-block;
  background: #FFFFFF;
  color: ##FFB100;
  padding: 2px 8px;
  border-radius: 5px;
} 
 <meta name="viewport" content="width=device-width" />
 </style>
    </head>  
    <body>  
    <table class="greenTable">
<thead>
<tr>
<th>Imię</th>
<th>Nazwisko</th>
<th>Email</th>
<th>Wypożyczony samochód</th>
<th>Numer telefonu</th>
<th>Data rozpoczęcia wypożyczenia</th>
<th>Data zakończenia wypożyczenia</th>
</tr>
</thead>
<tbody>
    @foreach (var item in Model)  
   {  
       <tr>  
           <td>@item.FirstName</td>  
           <td>@item.LastName</td>  
           <td>@item.Email</td>  
           <td>@item.RentalCar</td>    
           <td>@item.PhoneNumber</td>    
           <td>@item.RentalDate</td>    
           <td>@item.EndDate</td>    
        </tr>  
    }
    </tbody>
</table>
       </body>  
</html>
 